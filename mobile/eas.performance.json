{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "production-performance": {
      "extends": "production",
      "env": {
        "EXPO_USE_METRO_CONFIG": "metro.config.performance.js",
        "BABEL_CONFIG": "babel.config.performance.js",
        "NODE_ENV": "production",
        "PERFORMANCE_OPTIMIZATION": "true"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "cache": {
          "disabled": false,
          "customPaths": [
            "node_modules/.cache",
            ".expo"
          ]
        },
        "env": {
          "FLIPPER_DISABLE": "1"
        }
      },
      "ios": {
        "buildConfiguration": "Release",
        "cache": {
          "disabled": false,
          "customPaths": [
            "node_modules/.cache",
            ".expo",
            "ios/Pods"
          ]
        }
      }
    },
    "development-performance": {
      "extends": "development",
      "env": {
        "EXPO_USE_METRO_CONFIG": "metro.config.performance.js",
        "BABEL_CONFIG": "babel.config.performance.js",
        "NODE_ENV": "development",
        "PERFORMANCE_OPTIMIZATION": "true",
        "PERFORMANCE_MONITORING": "true"
      }
    },
    "preview-performance": {
      "extends": "preview",
      "env": {
        "EXPO_USE_METRO_CONFIG": "metro.config.performance.js",
        "BABEL_CONFIG": "babel.config.performance.js",
        "NODE_ENV": "production",
        "PERFORMANCE_OPTIMIZATION": "true"
      }
    }
  },
  "submit": {
    "production-performance": {
      "extends": "production",
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      }
    }
  }
}
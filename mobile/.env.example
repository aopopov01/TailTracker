# TailTracker Environment Variables

# Google Maps API Key (required for location features)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_URL=https://api.tailtracker.com

# Analytics (optional)
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
EXPO_PUBLIC_AMPLITUDE_API_KEY=your_amplitude_key_here

# Payment Processing (RevenueCat)
EXPO_PUBLIC_REVENUECAT_API_KEY=your_revenuecat_key_here
EXPO_PUBLIC_REVENUECAT_IOS_KEY=your_ios_key_here
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your_android_key_here

# Push Notifications
EXPO_PUBLIC_EXPO_PUSH_TOKEN=your_expo_push_token_here
{
  "name": "tailtracker-mobile",
  "version": "1.0.0",
  "description": "TailTracker - Pet Management Mobile App",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:android:local": "./scripts/build-android.sh",
    "submit:android": "eas submit --platform android",
    "build:ios": "eas build --platform ios",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:ios:simulator": "eas build --platform ios --profile simulator",
    "build:ios:local": "./scripts/build-ios.sh",
    "submit:ios": "eas submit --platform ios",
    "submit:ios:testflight": "eas submit --platform ios --profile testflight",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "test:accessibility": "jest --config jest.accessibility.config.js",
    "test:android": "detox test --configuration android",
    "test:ios": "detox test --configuration ios",
    "build:detox:android": "detox build --configuration android",
    "build:detox:ios": "detox build --configuration ios",
    "ios:pods": "cd ios && pod install",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:reset": "cd ios && rm -rf Pods && pod install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:ios:clean": "expo prebuild --platform ios --clean",
    "preflight": "./scripts/preflight-check.sh",
    "preflight:dev": "./scripts/preflight-check.sh --environment development",
    "preflight:staging": "./scripts/preflight-check.sh --environment staging",
    "preflight:production": "./scripts/preflight-check.sh --environment production",
    "version:bump": "./scripts/version-bump.sh",
    "version:patch": "./scripts/version-bump.sh --type patch",
    "version:minor": "./scripts/version-bump.sh --type minor",
    "version:major": "./scripts/version-bump.sh --type major",
    "setup:signing": "./scripts/setup-code-signing.sh",
    "build:analyze": "npx @next/bundle-analyzer",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && npm run ios:clean",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "postinstall": "patch-package",
    "device:setup": "./scripts/device-testing-setup.sh",
    "device:status": "./scripts/check-device-testing-status.sh",
    "device:test": "expo start",
    "device:tunnel": "expo start --tunnel",
    "device:clear": "expo start --clear",
    "device:localhost": "expo start --localhost"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "~1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@stripe/stripe-react-native": "^0.38.6",
    "@supabase/supabase-js": "^2.45.4",
    "expo": "~51.0.28",
    "expo-application": "~5.9.1",
    "expo-auth-session": "~5.5.2",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.9",
    "expo-image": "~1.12.15",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.28.15",
    "expo-router": "^5.1.4",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-task-manager": "~11.8.2",
    "expo-updates": "~0.25.24",
    "expo-web-browser": "~13.0.3",
    "framer-motion": "^12.23.12",
    "jimp": "^1.6.0",
    "react": "18.2.0",
    "react-hook-form": "^7.52.2",
    "react-native": "0.74.5",
    "react-native-biometrics": "^3.0.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-keychain": "^8.2.0",
    "react-native-maps": "1.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-permissions": "^4.1.5",
    "react-native-purchases": "^7.28.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-query": "^3.39.3",
    "sharp": "^0.34.3",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "detox": "^20.20.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/src/test/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/test/**/*"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
